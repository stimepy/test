a.tab {
	background-image: url(../../templates/Saphic/images/cellpic2.gif); 
	border-collapse: collapse;
	border-style: solid solid none solid;
	border-color: #A1BA25;
	border-width: 1px 1px 1px 1px;
	padding: 2px 1px 2px 1px;
	margin-top: 4px;
	margin-right:2px;
	font-family: arial;
	text-decoration: none;
	white-space:nowrap;
	color:#FFFFFF;
}
a.tab:hover {
	background-image: url(../../templates/Saphic/images/cellpic2.gif); 
	border-collapse: collapse;
	border-style: solid solid none solid;
	border-color: #A1BA25;
	border-width: 1px 1px 0px 1px;
	padding: 2px 1px 2px 1px;
	text-decoration: none;
	margin-right:2px;
	white-space:nowrap;
}
.panel { 
	border: solid 0px #979797; 
	padding: 0px; 
	overflow:visible;
	text-decoration: none;
}
td, th, p, li
{
	font: 10pt verdana, geneva, lucida, 'lucida grande', arial, helvetica, sans-serif;
}
.tborder
{
	border: solid 1px #A1BA25;

}
.title{
	background-image: url(../../templates/Saphic/images/cellpic1.gif); 
	
}
.thead
{
	color: #EEEEEE; 
	font-size: 10px; 
	font-weight : bold;
	background-image: url(../../templates/Saphic/images/cellpic2.gif); 
	height: 25px;
}

.tfoot
{
	color: #EEEEEE;
	background-image: url(../../templates/Saphic/images/cellpic2.gif);
}

.alt1, .alt1Active
{
	background-color: #FFFFFF;
}
.alt2, .alt2Active
{
	background-color: #F7F7F7; 

}
